{
  "name": "raspicam",
  "version": "1.1.0",
  "main": "src/server/index.ts",
  "browser": "src/site/index.tsx",
  "author": "Patrick Matt",
  "license": "MIT",
  "scripts": {
    "start:site": "webpack serve --mode development --config webpack.site.js",
    "start:server": "nodemon --watch src/server src/server/index.ts",
    "code:check": "npm run code:check-eslint && yarn code:check-tsc",
    "code:check-eslint": "eslint --fix src/",
    "code:check-tsc": "tsc -p ./tsconfig.json",
    "build": "npm run build:site && npm run build:server",
    "build:site": "webpack --config webpack.site.js",
    "build:server": "webpack --config webpack.server.js"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "chalk": "^4.1.2",
    "express": "^4.18.1",
    "jmuxer": "^2.0.4",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0",
    "stream-browserify": "^3.0.0",
    "styled-components": "^5.3.5",
    "styled-normalize": "^8.0.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.41",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.25",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "react-is": "^18.1.0",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3",
    "typescript-plugin-styled-components": "^2.0.0",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.2"
  }
